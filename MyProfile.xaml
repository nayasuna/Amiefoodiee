<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Amiefoodiee.OrderDetail">
    <ContentPage.Content>
        <StackLayout  BackgroundColor="#F5F5F5">
            <Image Source="Group63.png" Margin="6,-80,7,14" WidthRequest="361" HeightRequest="621"></Image>
            <ImageButton x:Name="Btnorderhistory" Source="OrderHistory.png" Margin="90,-50,46,30" BackgroundColor="Transparent"  WidthRequest="253" HeightRequest="43"
                          Clicked="Btnorderhistory_Clicked"></ImageButton>
            <ImageButton x:Name="Btnlanguage" Source="Language.png" Margin="90,-50,46,30" BackgroundColor="Transparent"  WidthRequest="253" HeightRequest="43"
                          Clicked="Btnlanguage_Clicked"></ImageButton>
            <ImageButton x:Name="Btnmyaddress" Source="Myaddress.png" Margin="90,-50,46,30" BackgroundColor="Transparent"  WidthRequest="253" HeightRequest="43"
                          Clicked="Btnmyaddress_Clicked"></ImageButton>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>